<?xml version="1.0"?>
<!-- Regional definitions for the Azores -->
<PropertyList>
	<name>Azores</name>

	<area>
		<lon1>-2.3</lon1>
		<lon2>0.1</lon2>
		<lat1>43.5</lat1>
		<lat2>42.3</lat2>
	</area>
	<area>
		<lon1>0.1</lon1>
		<lon2>3.5</lon2>
		<lat1>43.1</lat1>
		<lat2>42</lat2>
	</area>
	<condition>
		<equals>
			<property>sim/startup/season</property>
